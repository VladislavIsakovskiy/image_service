[flake8]
application_import_names = devops_as_a_service
max-line-length = 120
ignore = B008
#  I100  # There is need to knowledge about application package name and a lot of false positives
exclude =
    .git,
    .idea,
    .venv,
    .mypy_cache,
    .pytest_cache,
    __pycache__,
    devops_as_a_service/rnd_utils/security.py

